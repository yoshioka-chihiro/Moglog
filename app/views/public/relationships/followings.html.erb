<% if @end_user.is_deleted == true %>
  <p>このユーザーは退会済みです。</p>
<% else %>
  <div class="container">
    <h3 class="mini-title mb-3">フォロー</h3>
    <div class="justify-content-center row">
      <%= render 'follow_list', end_users: @end_users %>
    </div>
    <div class="row justify-content-center"><%=  link_to '戻る', end_user_path(@end_user) %></div>
  </div>
<% end %>