<h1>会員情報詳細</h1>
<table class="table">
  <tr>
    <th>名前</th>
    <td><%= @end_user.name %></td>
  </tr>
  <tr>
    <th>ニックネーム</th>
    <td><%= @end_user.nickname %></td>
  </tr>
  <tr>
    <th>性別</th>
    <td><%= @end_user.gender %></td>
  </tr>
  <tr>
    <th>スタート時体重</th>
    <td><%= @end_user.start_weight %></td>
  </tr>
  <tr>
    <th>目標体重</th>
    <td><%= @end_user.objective_weight %></td>
  </tr>
  <tr>
    <th>年齢</th>
    <td><%= @end_user.age %></td>
  </tr>
  <tr>
    <th>身長</th>
    <td><%= @end_user.height %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= @end_user.email %></td>
  </tr>
</table>

<%= link_to '編集',  edit_admin_end_user_path %>